        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.343" />

    <TextView
        android:id="@+id/days_away1"
        android:layout_width="106dp"
        android:layout_height="32dp"
        android:layout_gravity="center"
